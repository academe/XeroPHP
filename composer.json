{
    "name": "academe/xero-php",
    "type": "library",
    "description": "A client implementation of the Xero API, supplying raw data.",
    "homepage": "https://github.com/academe/XeroPHP",
    "license": "MIT",
    "keywords": ["xero", "api", "json", "guzzle", "rest-api", "xero-api", "oauth", "xero-provider"],
    "authors": [
        {
            "name": "Jason Judge",
            "email": "jason@academe.co.uk",
            "homepage": "https://academe.co.uk",
            "role": "Developer"
        },
        {
            "name": "Jason Judge",
            "email": "jason@consil.co.uk",
            "homepage": "https://consil.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/oauth-subscriber": "0.3.*",
        "nesbot/carbon": "^1.20"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": " ^2.8.1"
    },

    "suggest": {
        "thephpleague/oauth1-client": "Provides OAuth 1.0 authentication to your application",
        "invoiced/oauth1-xero": "Provides Xero-specific functionality to thephpleague/oauth1-client"
    },

    "repositories": [
    ],

    "autoload": {
        "psr-4": {
            "Academe\\XeroPHP\\": "src/"
        }
    }
}
